
{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
     "strict": true,
     "allowImportingTsExtensions": true,
     "skipLibCheck": true,
     "lib": ["dom", "dom.iterable", "esnext"],
     "types": ["react", "react-dom"]
  },
   "include": ["src/**/*"],
}
